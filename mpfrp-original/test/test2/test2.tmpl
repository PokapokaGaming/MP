out(pq, {A, B}) -> 
    io:format("outp: (~p, ~p)~n", [A, B]);
out(ppq, {A, B, C}) ->
    io:format("outq: (~p, ~p, ~p)~n", [A, B, C]);
out(ppqqr, {A, B, C, D, E}) ->
    io:format("outr: (~p, ~p, ~p, ~p, ~p)~n", [A, B, C, D, E]).

